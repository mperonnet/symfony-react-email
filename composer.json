{
    "name": "mperonnet/symfony-react-email",
    "description": "React email for Symfony",
    "keywords": ["symfony", "react", "react email"],
    "homepage": "https://github.com/mperonnet/symfony-react-email",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mperonnet\\ReactEmail\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Matteo Peronnet",
            "email": "matteo-peronnet@hotmail.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/process": "^5.4|^6.0|^7.0",
        "symfony/mailer": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.5.3",
        "pestphp/pest": "^1.20 || ^2.0 || ^3",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "pestphp/pest-plugin": true
        }
    }
}